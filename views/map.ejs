<%-include header%>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAoEGTfMYy0VAgBmvAGCTvnHVbKmbNI-IU"></script>

<script>
    var map;

    function initialize() {
        var lat = <%= result.geometry.location.lat%> ;
        var long = <%= result.geometry.location.lng%> ;
        console.log('lat', lat);
        console.log('long', long);
        var myLatlng = new google.maps.LatLng(lat,long);

        var mapOptions = {
            zoom: 11,
            center: new google.maps.LatLng(49.246292, -123.116226)
        };
        map = new google.maps.Map(document.getElementById('map-canvas'),
                mapOptions);
        var marker = new google.maps.Marker({
            position: myLatlng,
            map: map,
            title:"Hello World!"
        });
    }

    //initialize();
    console.log("in map.js");
    google.maps.event.addDomListener(window, 'load', initialize);
</script>


<div id="map-canvas"></div>
<%-include footer%>
